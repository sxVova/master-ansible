global:
  resolve_timeout: 1m

route:
  group_by: ['...']
  group_wait: 10s
  group_interval: 10s
  repeat_interval: 30m
  receiver: 'SRE-UA'

receivers:
- name: SRE
  pagerduty_configs:
  - service_key: ''

inhibit_rules:
  - source_match:
      severity: 'critical'
    target_match:
      severity: 'warning'
    equal: ['alertname', 'dev', 'instance']